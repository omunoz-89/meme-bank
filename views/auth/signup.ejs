<div class="card text-center" id="signUp">
  <div class="card-header">
    <span class="formTitle">Enter Info to Signup</span>
  </div>
  <div class="card-body">
<form action="/auth/signup" method="post">
<div class="signupRow row g-3">
  <div class="col">
    <input type="name" class="form-control" placeholder="First name" aria-label="First name" id="auth-first_name" name="first_name">
  </div>
  <div class="col">
    <input type="name" class="form-control" placeholder="Last name" aria-label="Last name" id="auth-last_name" name="last_name">
  </div>
</div>
<div class="signupRow row g-3">
  <div class="col">
    <input type="username" class="form-control" placeholder="Username" aria-label="Username" id="auth-user_name" name="user_name">
  </div>
</div>
<div class="signupRow row g-3">
  <div class="col">
    <input type="email" class="form-control" placeholder="Email" aria-label="Email" id="new-email" name="email">
  </div>
  <div class="col">
    <input type="password" class="form-control" placeholder="Password" aria-label="Password" id="new-password" name="password">
  </div>
</div>
<button type="submit" class="btn btn-primary">Sign Up</button>
</form>
</div>
</div>